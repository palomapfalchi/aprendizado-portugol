Algoritmo "ESCOLHA_COORDENADAS"


Var

   x, y : real
   coordenada, quadrante: caractere

Inicio

   escreva("Valor de x: ")
   leia(x)
   escreva("Valor de y: ")
   leia(y)
   
   escolha coordenada
      caso (x = 0) e (y = 0)
         quadrante <- "Origem"
      


   fimescolha
   
   escreva("Quadrante: ", quadrante)



Fimalgoritmo