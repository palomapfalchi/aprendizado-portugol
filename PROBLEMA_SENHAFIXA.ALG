Algoritmo "problema_senhafixa"

Var

   senha : inteiro


Inicio

   escreva("Digite a senha: ")
   leia(senha)
   
   enquanto senha <> 2002 faca

      se senha <> 2002 entao
         escreva("Senha invalida! tente novamente: ")
         leia(senha)
      senao
         escreva("Acesso permitido!")
      fimse
         
   fimenquanto

      escreva("Acesso permitido!")


Fimalgoritmo